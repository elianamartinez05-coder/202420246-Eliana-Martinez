#include <iostream>

using namespace std;

void counterLetter(string example1, string example2){


    string newString ="";
    string newString2 = "";
    int contador = 0;

    for(int i = 0; i < example.size(); i++)
    {
       char letter = example[i];
       for (int j = 0; j example2.size(); j++)
       {
        char letter2 = example2[j];
        if (letter == letter2)
        {
            contador ++;
        }
        
       }
       
    }
    cout <<"hay: " <<countador <<"letras" <<endl;

}

int main(){
    string example1;
    string example2;
    cin >> example1;
    cin >> example2
    counterLetter(example1, example2);
    return 0;
}